{
    "name": "prelude-so/laravel",
    "description": "Laravel integration package for prelude-so/sdk",
    "version": "1.0.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Tantimonaco",
            "email": "learning@andreatantimonaco.me"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "prelude-so/sdk": "^1.0",
        "laravel/framework": "^12.0",
        "orchestra/testbench": "^10.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "PreludeSo\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PreludeSo\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PreludeSo\\Laravel\\PreludeServiceProvider"
            ],
            "aliases": {
                "Prelude": "PreludeSo\\Laravel\\Facades\\Prelude"
            }
        }
    },
    "scripts": {
        "test": "pest tests/",
        "test-coverage": "pest --do-not-cache-result --coverage-clover=coverage.xml tests/"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
