{
    "name": "prelude-so/laravel",
    "description": "Laravel integration package for prelude-so/sdk",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Tantimonaco",
            "email": "learning@andreatantimonaco.me"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "prelude-so/sdk": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PreludeSo\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PreludeSo\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PreludeSo\\Laravel\\PreludeServiceProvider"
            ],
            "aliases": {
                "Prelude": "PreludeSo\\Laravel\\Facades\\Prelude"
            }
        }
    },
    "scripts": {
        "test": "pest",
        "test-coverage": "pest --coverage-html coverage"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
